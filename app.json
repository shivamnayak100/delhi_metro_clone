{
  "expo": {
    "name": "Delhi Metro App",
    "slug": "delhi-metro-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.delhi.metro.app",
      "versionCode": 1,
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "com.delhi.metro.app",
      "supportsTablet": true
    },
    "web": {
      "productionPath": "web-build"
    },
    "description": "Delhi Metro app I made for fun"
  }
}
